{
  "compilerOptions": {
    "allowSyntheticDefaultImports":true,
    "module": "CommonJS",
    "esModuleInterop": true,
    "target": "ES6",
    "moduleResolution": "node",
    "sourceMap": false,
    "rootDirs": [
      "src/server"
    ],
    "outDir": "../build",
    "strict": false,
    "allowJs": true,
    "skipLibCheck": true,
  },
  "lib": [
    "es2015"
  ],
  "exclude": ["./build"]
}